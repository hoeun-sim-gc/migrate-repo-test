{
  "version": "0.3.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint src stories --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -s public -p 6007",
    "build-storybook": "build-storybook -s public"
  },
  "peerDependencies": {
    "react": ">=16",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "jest-junit": {
    "suiteNameTemplate": "{filepath}",
    "outputDirectory": "tmp",
    "outputName": "junit.xml"
  },
  "prettier": {
    "printWidth": 96,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@gcui/react",
  "author": "Mr. Darcy Murphy",
  "module": "dist/gcui-react.esm.js",
  "size-limit": [
    {
      "path": "dist/gcui-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/gcui-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@size-limit/preset-small-lib": "^4.9.1",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/addons": "^6.1.14",
    "@storybook/react": "^6.1.14",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "babel-loader": "^8.2.2",
    "husky": "^4.3.8",
    "jest-junit": "^12.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/data-grid": "^4.0.0-alpha.27",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57"
  }
}
